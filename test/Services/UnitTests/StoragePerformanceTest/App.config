<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <runtime>
    <gcServer enabled="true" />
    <gcConcurrent enabled="true" />
  </runtime>
  <appSettings>
    <add key="storeRootPath" value="d:\storage-test"/>
    <add key="concurrentThreadCount" value="1"/>
    <add key="recordSize" value="1024"/>
    <add key="recordCount" value="50000000"/>
    <add key="syncFlush" value="false"/>
    <!-- FlushToOS, FlushToDisk -->
    <add key="flushOption" value="FlushToOS"/>
  </appSettings>
</configuration>